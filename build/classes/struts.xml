<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

	<package name="default" namespace="/" extends="struts-default">
		<action name="hello" class="com.sshenv.struts2.action.HelloAction">
			<result>/hello.jsp</result>
		</action>

		<action name="login"
			class="com.sshenv.struts2.action.UserAction" method="login">
		</action>

		<action name="postString"
			class="com.sshenv.struts2.action.PostStringAction"
			method="postString">
		</action>

		<action name="getSign" class="com.sshenv.gsww.getSignAction"
			method="getSign">
		</action>
	</package>

</struts>